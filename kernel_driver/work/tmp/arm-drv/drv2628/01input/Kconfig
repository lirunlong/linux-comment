menuconfig SHREK_INPUT
	bool "Driver for input devices"

if SHREK_INPUT

config KEYS01
	tristate "Input driver for Keys"
	---help---
	  standard input driver for shrek6410 board's keys

config TS01
	tristate "Input driver for Touchscreen"
	---help---
	  standard input driver for shrek6410 board's Touchscreen

endif #SHREK_INPUT

