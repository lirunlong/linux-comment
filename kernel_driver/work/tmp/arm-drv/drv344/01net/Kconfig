menuconfig SHREK_NET
	bool "Driver for network devices"

if SHREK_NET

config NET01
	tristate "Network driver for DM9000A"
	---help---
	  standard network driver for shrek6410 board's DM9000A ethernet chip

endif #SHREK_NET

